000) {\n        return true;\n      } else return false;\n    } catch (err) {\n      return false;\n    }\n  }\n\n  getToken() {\n    // Retrieves the user token from localStorage\n    return localStorage.getItem('id_token');\n  }\n\n  login(idToken) {\n    // Saves user token to localStorage\n    localStorage.setItem('id_token', idToken);\n\n    window.location.assign('/');\n  }\n\n  logout() {\n    // Clear user token and profile data from localStorage\n    localStorage.removeItem('id_token');\n    // this will reload the page and reset the state of the application\n    window.location.assign('/');\n  }\n}\n\nexport default new AuthService();\n","/Users/patrickdoyle/MIA-VIRT-FSF-PT-11-2022-U-LOLC/22-State/01-Activities/24-Stu_Stripe/Solved/client/src/utils/queries.js",[],[],"/Users/patrickdoyle/MIA-VIRT-FSF-PT-11-2022-U-LOLC/22-State/01-Activities/24-Stu_Stripe/Solved/client/src/components/Jumbotron/index.js",[],[],"/Users/patrickdoyle/MIA-VIRT-FSF-PT-11-2022-U-LOLC/22-State/01-Activities/24-Stu_Stripe/Solved/client/src/components/CategoryMenu/index.js",[],[],"/Users/patrickdoyle/MIA-VIRT-FSF-PT-11-2022-U-LOLC/22-State/01-Activities/24-Stu_Stripe/Solved/client/src/components/ProductList/index.js",[],[],"/Users/patrickdoyle/MIA-VIRT-FSF-PT-11-2022-U-LOLC/22-State/01-Activities/24-Stu_Stripe/Solved/client/src/components/Cart/index.js",[],[],"/Users/patrickdoyle/MIA-VIRT-FSF-PT-11-2022-U-LOLC/22-State/01-Activities/24-Stu_Stripe/Solved/client/src/components/ProductItem/index.js",[],[],"/Users/patrickdoyle/MIA-VIRT-FSF-PT-11-2022-U-LOLC/22-State/01-Activities/24-Stu_Stripe/Solved/client/src/components/CartItem/index.js",[],[],{"ruleId":"125","severity":1,"message":"126","line":45,"column":1,"nodeType":"127","endLine":45,"endColumn":34},"import/no-anonymous-default-export","Assign instance to a variable before exporting as module default","ExportDefaultDeclaration"]